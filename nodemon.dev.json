{
  "restartable": "rs",
  "ignore": [".git", "node_modules/", "dist/", "coverage/"],
  "watch": ["src/"],
  "execMap": {
    "ts": "node -r ts-node/register"
  },
  "env": {
    "NODE_ENV": "dev",
    "TEMPLATE_SERVICE_PORT": 8000,
    "TRACKER_SERVICE_PORT": 8001
  },
  "events": {
    "restart": "npm run build:snippet"
  },
  "ext": "ts,html"
}
